<tex2html_mydb_mark>#472#$f({\bf x}_{n+1})=f({\bf x}_n+\delta{\bf d}_n)$
<tex2html_mydb_mark>#473#\begin{equation}
f({\bf x}_{n+1})=f({\bf x}_n+\delta{\bf d}_n)
    \le f({\bf x}_n)+c_1\,\delta{\bf d}^T {\bf g}_n
\end{equation}
<tex2html_mydb_mark>#474#\begin{equation}
{\bf g}_{n+1}^T {\bf d}_n\ge c_2\;  {\bf g}_n^T {\bf d}_n
\end{equation}
<tex2html_mydb_mark>#475#${\bf g}_n^T{\bf d}_n<0$
<tex2html_mydb_mark>#476#\begin{equation}
|{\bf g}_{n+1}^T {\bf d}_n| < |c_2\;{\bf g}_n^T {\bf d}_n|
\end{equation}
<tex2html_mydb_mark>#477#$0 < c_1 < c_2 < 1$
<tex2html_mydb_mark>#478#$f({\bf x})$
<tex2html_mydb_mark>#479#$\phi(\delta)=f({\bf x}_n+\delta{\bf d}_n)$
<tex2html_mydb_mark>#480#$L_0(\delta)=a+b\delta$
<tex2html_mydb_mark>#481#$a=L_0(0)=\phi(\delta)\big|_{\delta=0}=f({\bf x}_n)$
<tex2html_mydb_mark>#482#$\phi(\delta)=f({\bf x}_n+\delta{\bf d}_n)$
<tex2html_mydb_mark>#483#\begin{equation}
b=\frac{d}{d\delta}\;f({\bf x}_n+\delta{\bf d}_n)\bigg|_{\delta=0}
  ={\bf g}^T_n{\bf d}_n<0
\end{equation}
<tex2html_mydb_mark>#484#$f({\bf x}_n+\delta{\bf d}_n)<f({\bf x}_n)$
<tex2html_mydb_mark>#485#\begin{equation}
L_0(\delta)=a+b\delta=f({\bf x}_n)+{\bf g}^T_n{\bf d}_n\delta
\end{equation}
<tex2html_mydb_mark>#486#$L_1(\delta)=f({\bf x}_n)$
<tex2html_mydb_mark>#487#$L_0(\delta)$
<tex2html_mydb_mark>#488#$L_1(\delta)$
<tex2html_mydb_mark>#489#$L(\delta)=f({\bf x}_n)+c_1\,{\bf g}^T_n{\bf d}_n\delta$
<tex2html_mydb_mark>#490#$0< c_1 {\bf g}_n^T{\bf d}_n< {\bf g}_n^T{\bf d}_n$
<tex2html_mydb_mark>#491#\begin{equation}
f({\bf x}_{n+1})=f({\bf x}_n+\delta{\bf d}_n)
  \le f({\bf x}_n)+c_1\,{\bf g}^T_n{\bf d}_n\delta <f({\bf x}_n)

\end{equation}
<tex2html_mydb_mark>#492#$f({\bf x})$
<tex2html_mydb_mark>#493#${\bf x}_{n+1}$
<tex2html_mydb_mark>#494#${\bf g}_{n+1}$
<tex2html_mydb_mark>#495#${\bf g}_{n+1}\perp{\bf g}_n$
<tex2html_mydb_mark>#496#$-{\bf g}_n$
<tex2html_mydb_mark>#497#$f(\delta_n)=f({\bf x}_n-\delta_n{\bf g}_n)$
<tex2html_mydb_mark>#498#${\bf x}_{n+1}$
<tex2html_mydb_mark>#499#$f(\delta_n)$
<tex2html_mydb_mark>#500#${\bf g}_{n+1}$
<tex2html_mydb_mark>#501#$-{\bf g}_n$
<tex2html_mydb_mark>#502#${\bf g}_{n+1}\perp {\bf g}_n$
<tex2html_mydb_mark>#503#${\bf g}_{n+1}^T{\bf g}_n=0$
<tex2html_mydb_mark>#504#\begin{equation}
{\bf x}_{n+1}={\bf x}_n-\delta_n{\bf g}_n+\alpha_n({\bf x}_n-{\bf x}_{n-1})
\end{equation}
<tex2html_mydb_mark>#505#\begin{equation}
f(x,y)=(a-x)^2+b(y-x^2)^2\;\;\;\;\;\;\;\;\mbox{typically $a=1,\;
    b=100$}
  \nonumber
\end{equation}
<tex2html_mydb_mark>#506#$f(1,\;1)=0$
<tex2html_mydb_mark>#507#${\bf f}({\bf x})={\bf0}$
<tex2html_mydb_mark>#508#\begin{equation}
{\bf x}_{n+1}={\bf x}_n-\delta {\bf J}({\bf x}_n)^{-1}{\bf f}({\bf x}_n)
    ={\bf x}_n-\delta\;{\bf J}_n^{-1}{\bf f}_n
  
\end{equation}
<tex2html_mydb_mark>#509#${\bf J}_n={\bf J}({\bf x}_n)$
<tex2html_mydb_mark>#510#${\bf f}({\bf x}_n)$
<tex2html_mydb_mark>#511#$f({\bf x})$
<tex2html_mydb_mark>#512#\begin{equation}
{\bf x}_{n+1}={\bf x}_n-\delta {\bf H}({\bf x}_n)^{-1}{\bf g}({\bf x}_n)
  ={\bf x}_n-\delta\;{\bf H}_n^{-1}{\bf g}_n
\end{equation}
<tex2html_mydb_mark>#513#${\bf g}_n={\bf g}({\bf x}_n)$
<tex2html_mydb_mark>#514#${\bf H}_n={\bf H}({\bf x}_n)$
<tex2html_mydb_mark>#515#$f({\bf x})$
<tex2html_mydb_mark>#516#${\bf J}({\bf x})$
<tex2html_mydb_mark>#517#${\bf H}({\bf x})$
<tex2html_mydb_mark>#518#$f({\bf x})$
<tex2html_mydb_mark>#519#$f({\bf x})$
<tex2html_mydb_mark>#520#${\bf x}_{n+1}$
<tex2html_mydb_mark>#521#\begin{equation}
f({\bf x})=f({\bf x}_{n+1})+({\bf x}-{\bf x}_{n+1})^T {\bf g}_{n+1}
  +\frac{1}{2}({\bf x}-{\bf x}_{n+1})^T {\bf H}_{n+1}
  ({\bf x}-{\bf x}_{n+1})+O(||{\bf x}-{\bf x}_{n+1}||^3)
\end{equation}
<tex2html_mydb_mark>#522#\begin{equation}
\frac{d}{d{\bf x}}f({\bf x})={\bf g}({\bf x})
  ={\bf g}_{n+1}+{\bf H}_{n+1} ({\bf x}-{\bf x}_{n+1})
  +O(||{\bf x}-{\bf x}_{n+1}||^2)
\end{equation}
<tex2html_mydb_mark>#523#${\bf x}={\bf x}_n$
<tex2html_mydb_mark>#524#${\bf g}({\bf x}_n)={\bf g}_n$
<tex2html_mydb_mark>#525#\begin{eqnarray}
{\bf g}_{n+1}-{\bf g}_n&=&{\bf H}_{n+1}({\bf x}_{n+1}-{\bf x}_n)
  +O(||{\bf x}_{n+1}-{\bf x}_n||^2)
  \nonumber\\
  &=&{\bf B}_{n+1}({\bf x}_{n+1}-{\bf x}_n)

\end{eqnarray}
<tex2html_mydb_mark>#526#\begin{equation}
{\bf s}_n={\bf x}_{n+1}-{\bf x}_n,\;\;\;\;\;\;
{\bf y}_n={\bf g}_{n+1}-{\bf g}_n
\end{equation}
<tex2html_mydb_mark>#527#\begin{equation}
{\bf B}_{n+1}{\bf s}_n={\bf y}_n,\;\;\;\;\;\mbox{or}\;\;\;\;\;
  {\bf B}_{n+1}^{-1}{\bf y}_n={\bf s}_n
\end{equation}
<tex2html_mydb_mark>#528#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#529#${\bf d}_n=-{\bf B}_n^{-1}{\bf g}_n$
<tex2html_mydb_mark>#530#${\bf x}_{n+1}={\bf x}_n+\delta {\bf d}_n$
<tex2html_mydb_mark>#531#${\bf B}_{n+1}={\bf B}_n+\Delta{\bf B}_n$
<tex2html_mydb_mark>#532#${\bf B}_{n+1}^{-1}={\bf B}_n^{-1}+\Delta{\bf B}_n^{-1}$
<tex2html_mydb_mark>#533#$f({\bf x})$
<tex2html_mydb_mark>#534#${\bf B}_n={\bf I}$
<tex2html_mydb_mark>#535#${\bf d}_n=-{\bf g}_n$
<tex2html_mydb_mark>#536#${\bf B}_n={\bf H}_n$
<tex2html_mydb_mark>#537#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#538#\begin{equation}
{\bf B}_{n+1}={\bf B}_n+{\bf uu}^T
\end{equation}
<tex2html_mydb_mark>#539#${\bf uu}^T$
<tex2html_mydb_mark>#540#\begin{equation}
{\bf B}_{n+1}{\bf s}_n
    ={\bf B}_n{\bf s}_n+{\bf uu}^T{\bf s}_n={\bf y}_n,
    \;\;\;\;\mbox{i.e.,}\;\;\;\;\;
    {\bf u}({\bf u}^T{\bf s}_n)={\bf y}_n-{\bf B}_n{\bf s}_n
\end{equation}
<tex2html_mydb_mark>#541#${\bf w}={\bf y}_n-{\bf B}_n{\bf s}_n$
<tex2html_mydb_mark>#542#${\bf u}=c {\bf w}$
<tex2html_mydb_mark>#543#\begin{equation}
{\bf u}{\bf u}^T{\bf s}_n=c^2{\bf w}{\bf w}^T{\bf s}_n={\bf w}
\end{equation}
<tex2html_mydb_mark>#544#$c =1/({\bf w}^T {\bf s}_n)^{1/2}$
<tex2html_mydb_mark>#545#\begin{equation}
{\bf u}=c{\bf w}=\frac{{\bf y}_n-{\bf B}_n{\bf s}_n}{({\bf w}^T{\bf s}_n)^{1/2}}
    =\frac{{\bf y}_n-{\bf B}_n{\bf s}_n}{( ({\bf y}_n-{\bf B}_n{\bf s}_n)^T{\bf s}_n)^{1/2}}
\end{equation}
<tex2html_mydb_mark>#546#\begin{equation}
{\bf B}_{n+1}={\bf B}_n+{\bf uu}^T
    ={\bf B}_n
    +\frac{ ({\bf y}_n-{\bf B}_n{\bf s}_n)({\bf y}_n-{\bf B}_n{\bf s}_n)^T }
    { ({\bf y}_n-{\bf B}_n{\bf s}_n)^T {\bf s}_n }
\end{equation}
<tex2html_mydb_mark>#547#${\bf B}_{n+1}$
<tex2html_mydb_mark>#548#\begin{eqnarray}
{\bf B}_{n+1}^{-1}&=&\left( {\bf B}_n+{\bf uu}^T \right)^{-1}
    ={\bf B}_n^{-1}-\frac{ {\bf B}_n^{-1}{\bf uu}^T{\bf B}_n^{-1}}{1+{\bf u}^T{\bf B}_n^{-1}{\bf u}}
    \nonumber\\
    &=&{\bf B}_n^{-1}
    +\frac{ ({\bf s}_n-{\bf B}_n^{-1}{\bf y}_n)({\bf s}_n-{\bf B}_n^{-1}{\bf y}_n)^T }
    { ({\bf s}_n-{\bf B}_n^{-1}{\bf y}_n)^T {\bf y}_n }
\end{eqnarray}
<tex2html_mydb_mark>#549#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#550#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#551#\begin{equation}
{\bf B}_{n+1}^{-1}={\bf B}_n^{-1}+{\bf uu}^T
\end{equation}
<tex2html_mydb_mark>#552#\begin{equation}
{\bf B}_{n+1}^{-1}{\bf y}_n
    ={\bf B}_n^{-1}{\bf y}_n+{\bf uu}^T{\bf y}_n={\bf s}_n,
\end{equation}
<tex2html_mydb_mark>#553#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#554#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#555#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#556#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#557#\begin{equation}
{\bf w}^T{\bf y}_n=({\bf s}_n-{\bf B}_n^{-1}{\bf y}_n)^T {\bf y}_n
  ={\bf s}_n^T{\bf y}_n -{\bf y}_n^T {\bf B}_n^{-1}{\bf y}_n >0
  
\end{equation}
<tex2html_mydb_mark>#558#\begin{equation}
{\bf B}_{n+1}={\bf B}_n+\alpha{\bf uu}^T+\beta{\bf vv}^T
\end{equation}
<tex2html_mydb_mark>#559#\begin{equation}
{\bf B}_{n+1} {\bf s}_n
    =({\bf B}_n+\alpha{\bf uu}^T+\beta{\bf vv}^T){\bf s}_n
    ={\bf B}_n{\bf s}_n+{\bf u}(\alpha{\bf u}^T{\bf s}_n)
    +{\bf v}(\beta{\bf v}^T{\bf s}_n)={\bf y}_n
\end{equation}
<tex2html_mydb_mark>#560#\begin{equation}
{\bf u}(\alpha{\bf u}^T{\bf s}_n)
    +{\bf v}(\beta{\bf v}^T{\bf s}_n)={\bf y}_n-{\bf B}_n{\bf s}_n
\end{equation}
<tex2html_mydb_mark>#561#\begin{equation}
{\bf u}={\bf y}_n,\;\;\;\;\;\;\alpha=\frac{1}{{\bf s}_n^T{\bf y}_n},
    \;\;\;\;\;{\bf v}={\bf B}_n{\bf s}_n,\;\;\;\;\;
    \beta=-\frac{1}{{\bf v}^T{\bf s}_n}=-\frac{1}{{\bf s}_n^T{\bf B}_n{\bf s}_n}
\end{equation}
<tex2html_mydb_mark>#562#${\bf B}_{n+1}={\bf B}_n+\alpha{\bf uu}^T+\beta{\bf vv}^T$
<tex2html_mydb_mark>#563#\begin{equation}
{\bf B}_{n+1}={\bf B}_n+\alpha{\bf uu}^T+\beta{\bf vv}^T
    ={\bf B}_n+\frac{{\bf y}_n{\bf y}_n^T}{{\bf y}_n^T{\bf s}_n}
    -\frac{{\bf B}_n{\bf s}_n{\bf s}_n^T{\bf B}_n}{{\bf s}_n^T{\bf B}_n{\bf s}_n}
\end{equation}
<tex2html_mydb_mark>#564#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#565#${\bf d}_n=-{\bf B}_n^{-1}{\bf g}_n$
<tex2html_mydb_mark>#566#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#567#${\bf U}=[{\bf u}_1\;{\bf u}_2]$
<tex2html_mydb_mark>#568#${\bf V}=[{\bf v}_1\;{\bf v}_2]$
<tex2html_mydb_mark>#569#\begin{equation}
{\bf u}_1={\bf v}_1=\frac{{\bf y}_n}{({\bf s}_n^T{\bf y}_n)^{1/2}},
    \;\;\;\;\;\;
    {\bf u}_2=-{\bf v}_2=\frac{{\bf B}_n{\bf s}_n}{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2}}
\end{equation}
<tex2html_mydb_mark>#570#\begin{equation}
{\bf B}_{n+1}={\bf B}_n+{\bf u}_1{\bf v}_1^T+{\bf u}_2{\bf v}_2^T
    =({\bf B}_n+{\bf UV}^T)^{-1}
\end{equation}
<tex2html_mydb_mark>#571#\begin{eqnarray}
{\bf B}_{n+1}^{-1}&=&({\bf B}_n+{\bf UV}^T)^{-1}
    ={\bf B}_n^{-1}-{\bf B}_n^{-1}{\bf U}({\bf I}+{\bf V}^T{\bf B}_n^{-1}{\bf U})^{-1}{\bf V}^T{\bf B}_n^{-1}
    \nonumber\\
    &=&{\bf B}_n^{-1}-{\bf B}_n^{-1}{\bf U}{\bf C}^{-1}{\bf V}^T{\bf B}_n^{-1}
    ={\bf B}_n^{-1}-{\bf B}_n^{-1}[{\bf u}_1\;{\bf u}_2]{\bf C}^{-1}
    ({\bf B}_n^{-1}[{\bf v}_1\;{\bf v}_2])^T
  
\end{eqnarray}
<tex2html_mydb_mark>#572#\begin{equation}
{\bf C}=\left[\begin{array}{cc}c_{11} & c_{12}\\c_{21} & c_{22}\end{array}\right]
    ={\bf I}+{\bf V}^T{\bf B}_n^{-1}{\bf U}
    ={\bf I}+[{\bf v}_1\;{\bf v}_2]^T{\bf B}^{-1}_n[{\bf u}_1\;{\bf u}_2]
\end{equation}
<tex2html_mydb_mark>#573#\begin{equation}
c_{11}=1+{\bf v}^T_1{\bf B}_n^{-1}{\bf u}_1
  =1+\frac{{\bf y}^T_n{\bf B}_n^{-1}{\bf y}_n}{{\bf s}_n^T{\bf y}_n}
\end{equation}
<tex2html_mydb_mark>#574#\begin{equation}
c_{22}=1+{\bf v}_2{\bf B}_n^{-1}{\bf u}_2
  =1-\frac{{\bf s}_n^T{\bf B}_n{\bf B}_n^{-1}{\bf B}_n{\bf s}_n}{{\bf s}_n^T{\bf B}_n{\bf s}_n}
  =0
\end{equation}
<tex2html_mydb_mark>#575#\begin{equation}
c_{12}={\bf v}_1^T{\bf B}_n^{-1}{\bf u}_2
  =\frac{{\bf y}_n^T{\bf B}_n^{-1}{\bf B}_n{\bf s}_n}
  { ({\bf s}_n^T{\bf y}_n)^{1/2} ({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2} }
  =\frac{({\bf s}_n^T{\bf y}_n)^{1/2}}{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2} }
\end{equation}
<tex2html_mydb_mark>#576#\begin{equation}
c_{21}={\bf v}_2^T{\bf B}_n^{-1}{\bf u}_1=-c_{12}
\end{equation}
<tex2html_mydb_mark>#577#\begin{equation}
{\bf C}=\left[\begin{array}{cc}c_{11} & c_{12}\\-c_{12} & 0\end{array}\right],
    \;\;\;\;\;\;\;\;\;\;\;\;
    {\bf C}^{-1}=
    \left[\begin{array}{cc}0 & -1/c_{12}\\1/c_{12} & c_{11}/c_{12}^2\end{array}\right]
\end{equation}
<tex2html_mydb_mark>#578#${\bf C}^{-1}$
<tex2html_mydb_mark>#579#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#580#\begin{eqnarray}
{\bf B}_{n+1}^{-1}&=&
    {\bf B}_n^{-1}-[{\bf B}_n^{-1}{\bf u}_1\;{\bf B}_n^{-1}{\bf u}_2]
    \left[\begin{array}{cc}0 & -1/c_{12}\\1/c_{12} & c_{11}/c_{12}^2\end{array}\right]
    [{\bf B}_n^{-1}{\bf v}_1\;{\bf B}_n^{-1}{\bf v}_2]^T
    \nonumber\\
    &=& {\bf B}_n^{-1}-\frac{1}{c_{12}}
        [{\bf B}_n^{-1}{\bf u}_2{\bf v}_1^T{\bf B}_n^{-1}
          -{\bf B}_n^{-1}{\bf u}_1{\bf v}_2^T{\bf B}_n^{-1}]
        -\frac{c_{11}}{c_{12}^2}{\bf B}_n^{-1}{\bf u}_2{\bf v}_2^T{\bf B}_n^{-1}
\end{eqnarray}
<tex2html_mydb_mark>#581#\begin{equation}
\frac{1}{c_{12}}{\bf B}_n^{-1}{\bf u}_2{\bf v}_1^T{\bf B}_n^{-1}
  =\frac{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2} }{({\bf s}_n^T{\bf y}_n)^{1/2}}
  {\bf B}_n^{-1}
  \frac{{\bf B}_n{\bf s}_n}{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2}}
  \frac{{\bf y}^T_n}{({\bf s}_n^T{\bf y}_n)^{1/2}} {\bf B}_n^{-1}
  =\frac{{\bf s}_n{\bf y}_n^T{\bf B}_n^{-1}}{{\bf s}_n^T{\bf y}_n}
\end{equation}
<tex2html_mydb_mark>#582#\begin{equation}
-\frac{1}{c_{12}}{\bf B}_n^{-1}{\bf u}_1{\bf v}_2^T{\bf B}_n^{-1}
  =\frac{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2} }{({\bf s}_n^T{\bf y}_n)^{1/2}}
  {\bf B}_n^{-1}
  \frac{{\bf y}_n}{({\bf s}_n^T{\bf y}_n)^{1/2}}
  \frac{{\bf s}_n^T {\bf B}_n}{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2}}{\bf B}_n^{-1}
  =\frac{{\bf B}_n^{-1}{\bf y}_n{\bf s}_n^T}{{\bf s}_n^T{\bf y}_n}
\end{equation}
<tex2html_mydb_mark>#583#\begin{eqnarray}
\frac{c_{11}}{c_{12}^2}{\bf B}_n^{-1}{\bf u}_2{\bf v}_2^T{\bf B}_n^{-1}
    &=&-\left(1+\frac{{\bf y}^T_n{\bf B}_n^{-1}{\bf y}_n}{{\bf s}_n^T{\bf y}_n}\right)
    \frac{{\bf s}_n^T{\bf B}_n{\bf s}_n}{{\bf s}_n^T{\bf y}_n} {\bf B}_n^{-1}
    \frac{{\bf B}_n{\bf s}_n}{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2}}
    \frac{{\bf s}_n^T {\bf B}_n}{({\bf s}_n^T{\bf B}_n{\bf s}_n)^{1/2}} {\bf B}_n^{-1}
    \nonumber\\
    &=&-\left(1+\frac{{\bf y}^T_n{\bf B}_n^{-1}{\bf y}_n}{{\bf s}_n^T{\bf y}_n}\right)
    \frac{{\bf s}_n{\bf s}_n^T}{{\bf s}_n^T{\bf y}_n}
\end{eqnarray}
<tex2html_mydb_mark>#584#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#585#\begin{equation}
{\bf B}_{n+1}^{-1}=
    {\bf B}_n^{-1}-\frac{{\bf B}_n^{-1}{\bf y}_n{\bf s}_n^T+{\bf s}_n{\bf y}_n^T{\bf B}_n^{-1}}{{\bf s}_n^T{\bf y}_n}
    +\left(1+\frac{{\bf y}^T_n{\bf B}_n^{-1}{\bf y}_n}{{\bf s}_n^T{\bf y}_n}\right)
    \frac{{\bf s}_n{\bf s}_n^T}{{\bf s}_n^T{\bf y}_n}
\end{equation}
<tex2html_mydb_mark>#586#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#587#${\bf B}_{n+1}$
<tex2html_mydb_mark>#588#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#589#${\bf B}^{-1}_n$
<tex2html_mydb_mark>#590#\begin{equation}
{\bf B}^{-1}_{n+1}={\bf B}_n^{-1}+\alpha{\bf uu}^T+\beta{\bf vv}^T
\end{equation}
<tex2html_mydb_mark>#591#\begin{equation}
{\bf B}_{n+1}^{-1} {\bf y}_n
    =({\bf B}_n^{-1}+\alpha{\bf uu}^T+\beta{\bf vv}^T){\bf y}_n
    ={\bf B}_n^{-1}{\bf y}_n+{\bf u}(\alpha{\bf u}^T{\bf y}_n)
    +{\bf v}(\beta{\bf v}^T{\bf y}_n)={\bf s}_n
\end{equation}
<tex2html_mydb_mark>#592#\begin{equation}
{\bf u}(\alpha{\bf u}^T{\bf y}_n)
    +{\bf v}(\beta{\bf v}^T{\bf y}_n)={\bf s}_n-{\bf B}_n^{-1}{\bf y}_n
\end{equation}
<tex2html_mydb_mark>#593#\begin{equation}
{\bf u}={\bf s}_n,\;\;\;\;\;\;\alpha=\frac{1}{{\bf s}^T{\bf y}_n},
    \;\;\;\;\;{\bf v}={\bf B}_n^{-1}{\bf y}_n,\;\;\;\;\;
    \beta=-\frac{1}{{\bf v}^T{\bf y}_n}=-\frac{1}{{\bf y}_n^T{\bf B}_n^{-1}{\bf y}_n}
\end{equation}
<tex2html_mydb_mark>#594#${\bf B}^{-1}_{n+1}={\bf B}^{-1}_n+\alpha{\bf uu}^T+\beta{\bf vv}^T$
<tex2html_mydb_mark>#595#\begin{equation}
{\bf B}^{-1}_{n+1}={\bf B}^{-1}_n+\alpha{\bf uu}^T+\beta{\bf vv}^T
    ={\bf B}^{-1}_n+\frac{{\bf s}_n{\bf s}_n^T}{{\bf s}_n^T{\bf y}_n}
    -\frac{{\bf B}_n^{-1}{\bf y}_n{\bf y}_n^T{\bf B}_n^{-1}}{{\bf y}_n^T{\bf B}_n^{-1}{\bf y}_n}
\end{equation}
<tex2html_mydb_mark>#596#\begin{equation}
{\bf B}_{n+1}=
    {\bf B}_n-\frac{{\bf B}_n{\bf s}_n{\bf y}_n^T+{\bf y}_n{\bf s}_n^T{\bf B}_n}{{\bf y}_n^T{\bf s}_n}
    +\left(1+\frac{{\bf s}^T_n{\bf B}_n{\bf s}_n}{{\bf y}_n^T{\bf s}_n}\right)
    \frac{{\bf y}_n{\bf y}_n^T}{{\bf y}_n^T{\bf s}_n}
\end{equation}
<tex2html_mydb_mark>#597#${\bf B}_n^{-1}$
<tex2html_mydb_mark>#598#${\bf z}^T{\bf B}_n{\bf z} > 0$
<tex2html_mydb_mark>#599#${\bf z}\ne{\bf0}$
<tex2html_mydb_mark>#600#\begin{equation}
{\bf g}_{n+1}^T {\bf d}_n \ge c_2\;{\bf g}_n^T {\bf d}_n
  \;\;\;\;\;\;\;\mbox{i.e.}\;\;\;\;\;
  \left( {\bf g}_{n+1}-c_2\;{\bf g}_n\right)^T {\bf d}_n \ge 0
\end{equation}
<tex2html_mydb_mark>#601#$\delta{\bf d}_n={\bf x}_{n+1}-{\bf x}_n={\bf s}_n$
<tex2html_mydb_mark>#602#\begin{equation}
({\bf g}_{n+1}-c_2{\bf g}_n)^T {\bf s}_n
={\bf g}^T_{n+1}{\bf s}_n-c_2{\bf g}_n^T {\bf s}_n \ge 0

\end{equation}
<tex2html_mydb_mark>#603#\begin{equation}
{\bf y}_n^T{\bf s}_n=({\bf g}_{n+1}-{\bf g}_n)^T {\bf s}_n
={\bf g}_{n+1}^T{\bf s}_n-{\bf g}_n^T {\bf s}_n
\end{equation}
<tex2html_mydb_mark>#604#\begin{equation}
{\bf y}_n^T{\bf s}_n-
  ({\bf g}^T_{n+1}{\bf s}_n-c_2{\bf g}_n^T {\bf s}_n)
  =(c_2-1){\bf g}_n^T{\bf s}_n > 0
\end{equation}
<tex2html_mydb_mark>#605#${\bf g}_n^T{\bf s}_n<0$
<tex2html_mydb_mark>#606#\begin{equation}
{\bf y}_n^T{\bf s}_n\ge {\bf g}^T_{n+1}{\bf s}_n-c_2{\bf g}_n^T {\bf s}_n \ge 0
\end{equation}
<tex2html_mydb_mark>#607#${\bf y}_n^T{\bf s}_n>0$
<tex2html_mydb_mark>#608#${\bf B}_{n+1}$
<tex2html_mydb_mark>#609#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#610#${\bf B}_n={\bf LL}^T$
<tex2html_mydb_mark>#611#${\bf a}={\bf L}^T{\bf z}$
<tex2html_mydb_mark>#612#${\bf b}={\bf L}^T{\bf s}$
<tex2html_mydb_mark>#613#\begin{equation}
{\bf a}^T{\bf a}={\bf z}^T{\bf B}_n{\bf z},\;\;\;\;
{\bf b}^T{\bf b}={\bf s}^T{\bf B}_n{\bf s},\;\;\;\;
{\bf a}^T{\bf b}={\bf z}^T{\bf B}_n{\bf s}
\end{equation}
<tex2html_mydb_mark>#614#\begin{equation}
{\bf z}^T\left[ {\bf B}_n-\frac{{\bf B}_n{\bf s}_n{\bf s}_n^T{\bf B}_n}{{\bf s}_n^T{\bf B}_n{\bf s}_n}\right] {\bf z}
  ={\bf z}^T{\bf B}_n{\bf z}
  -\frac{({\bf z}^T{\bf B}_n{\bf s}_n)^2}{{\bf s}_n^T{\bf B}_n{\bf s}_n}
  ={\bf a}^T{\bf a}-\frac{({\bf a}^T{\bf b})^2}{{\bf b}^T{\bf b}}\ge 0
\end{equation}
<tex2html_mydb_mark>#615#${\bf s}_n^T{\bf y}_n\ge 0$
<tex2html_mydb_mark>#616#\begin{equation}
{\bf z}^T\left(\frac{{\bf s}_n{\bf s}_n^T}{{\bf s}_n^T{\bf y}_n}\right){\bf z}\ge 0
\end{equation}
<tex2html_mydb_mark>#617#\begin{equation}
{\bf z}^T{\bf B}_{n+1}{\bf z}
=  {\bf z}^T\left( {\bf B}_n-\frac{{\bf B}_n{\bf s}_n{\bf s}_n^T{\bf B}_n}{{\bf s}_n^T{\bf B}_n{\bf s}_n}\right) {\bf z}
+{\bf z}^T\left(\frac{{\bf y}_n{\bf y}_n^T}{{\bf s}_n^T{\bf y}_n}\right){\bf z}\ge 0
\end{equation}
<tex2html_mydb_mark>#618#${\bf B}_{n+1}$
<tex2html_mydb_mark>#619#${\bf B}_{n+1}^{-1}$
<tex2html_mydb_mark>#620#$f({\bf x})$
<tex2html_mydb_mark>#621#\begin{equation}
f({\bf x})=\frac{1}{2}{\bf x}^T{\bf A}{\bf x}-{\bf b}^T{\bf x}+c
\end{equation}
<tex2html_mydb_mark>#622#${\bf A}={\bf A}^T$
<tex2html_mydb_mark>#623#$f({\bf x})$
<tex2html_mydb_mark>#624#\begin{equation}
{\bf g}({\bf x})=\frac{d}{d{\bf x}}f({\bf x})
  =\frac{d}{d{\bf x}}\left(\frac{1}{2}{\bf x}^T{\bf A}{\bf x}
  -{\bf b}^T{\bf x}+c\right)  ={\bf A}{\bf x}-{\bf b}
\end{equation}
<tex2html_mydb_mark>#625#\begin{equation}
{\bf H}({\bf x})=\frac{d^2}{d{\bf x}^2}f({\bf x})=\frac{d}{d{\bf x}}{\bf g}
  =\frac{d}{d{\bf x}}({\bf Ax}-{\bf b})={\bf A}
\end{equation}
<tex2html_mydb_mark>#626#$f({\bf x})$
<tex2html_mydb_mark>#627#${\bf g}({\bf x})={\bf A}{\bf x}-{\bf b}={\bf0}$
<tex2html_mydb_mark>#628#${\bf x}^*={\bf A}^{-1}{\bf b}$
<tex2html_mydb_mark>#629#\begin{equation}
f({\bf x}^*)=\frac{1}{2}({\bf A}^{-1}{\bf b})^T{\bf A}({\bf A}^{-1}{\bf b})
  -{\bf b}^T({\bf A}^{-1}{\bf b})+c=-\frac{1}{2}{\bf b}^T{\bf A}^{-1}{\bf b}+c
\end{equation}
<tex2html_mydb_mark>#630#$f({\bf x})$
<tex2html_mydb_mark>#631#${\bf g}({\bf x}^*)={\bf A}{\bf x}^*-{\bf b}={\bf0}$
<tex2html_mydb_mark>#632#${\bf g}_n={\bf g}({\bf x}_n)$
<tex2html_mydb_mark>#633#$\varepsilon=||{\bf g}_n||^2$
<tex2html_mydb_mark>#634#$f({\bf x})$
<tex2html_mydb_mark>#635#${\bf A}{\bf x}={\bf b}$
<tex2html_mydb_mark>#636#${\bf A}={\bf A}^T$
<tex2html_mydb_mark>#637#\begin{equation}
{\bf u}^T{\bf A}{\bf v}=({\bf A}{\bf v})^T{\bf u}
  ={\bf v}^T{\bf A}{\bf u}=0
\end{equation}
<tex2html_mydb_mark>#638#${\bf A}={\bf I}$
<tex2html_mydb_mark>#639#${\bf v}^T{\bf u}=0$
<tex2html_mydb_mark>#640#$\{{\bf d}_0,\cdots,{\bf d}_{N-1}\}$
<tex2html_mydb_mark>#641#${\bf d}_i^T{\bf A}{\bf d}_j=0$
<tex2html_mydb_mark>#642#\begin{equation}
{\bf v}_1=\left[\begin{array}{r}1\\0\end{array}\right],\;\;\;\;\;
  {\bf v}_2=\left[\begin{array}{r}0\\1\end{array}\right],\;\;\;\;\;
  {\bf A}=\left[\begin{array}{rr}3&1\\1&2\end{array}\right]
  \nonumber
\end{equation}
<tex2html_mydb_mark>#643#\begin{equation}
{\bf u}_1={\bf v}_1=\left[\begin{array}{r}1\\0\end{array}\right],
  \;\;\;\;\;\;\;\;
  {\bf u}_2={\bf v}_2-\frac{{\bf u}_1^T{\bf A}{\bf v}_2}{{\bf u}_1^T{\bf A}{\bf u}_1}{\bf u}_1
  =\left[\begin{array}{c}-1/3\\1\end{array}\right]
  \nonumber
\end{equation}
<tex2html_mydb_mark>#644#${\bf x}=[2,\;3]^T$
<tex2html_mydb_mark>#645#\begin{equation}
{\bf p}_{{\bf v}_1}({\bf x})=\frac{{\bf v}_1^T{\bf x}}{{\bf v}_1^T{\bf v}_1}{\bf v}_1
  =2\left[\begin{array}{c}1\\0\end{array}\right]
  =\left[\begin{array}{c}2\\0\end{array}\right],\;\;\;\;\;
  {\bf p}_{{\bf v}_2}({\bf x})=\frac{{\bf v}_2^T{\bf x}}{{\bf v}_2^T{\bf v}_2}{\bf v}_2
  =3\left[\begin{array}{c}0\\1\end{array}\right]
  =\left[\begin{array}{c}0\\3\end{array}\right]
  \nonumber
\end{equation}
<tex2html_mydb_mark>#646#\begin{equation}
{\bf p}_{{\bf u}_1}({\bf x})=\frac{{\bf u}_1^T{\bf A}{\bf x}}
  {{\bf u}_1^T{\bf A}{\bf u}_1}{\bf u}_1
  =3\left[\begin{array}{c}1\\0\end{array}\right]
  =\left[\begin{array}{c}3\\0\end{array}\right],
  \;\;\;\;\;\;\;\;\;
  {\bf p}_{{\bf u}_2}({\bf x})=\frac{{\bf u}_2^T{\bf A}{\bf x}}
  {{\bf u}_2^T{\bf A}{\bf u}_2}{\bf u}_2
  =3\left[\begin{array}{c}-1/3\\1\end{array}\right]
  =\left[\begin{array}{c}-1\\3\end{array}\right]
  \nonumber
\end{equation}
<tex2html_mydb_mark>#647#\begin{eqnarray}
{\bf x}=\left[\begin{array}{c}2\\3\end{array}\right]
  &=&2\left[\begin{array}{c}1\\0\end{array}\right]
  +3\left[\begin{array}{c}0\\1\end{array}\right]
  =2{\bf v}_1+3{\bf v}_2
  ={\bf p}_{{\bf v}_1}({\bf x})+{\bf p}_{{\bf v}_2}({\bf x})
  \nonumber \\
  &=&3\left[\begin{array}{c}1\\0\end{array}\right]
  +3\left[\begin{array}{c}-1/3\\1\end{array}\right]
  =3{\bf u}_1+3{\bf u}_2
  ={\bf p}_{{\bf u}_1}({\bf x})+{\bf p}_{{\bf u}_2}({\bf x})
  \nonumber
\end{eqnarray}
<tex2html_mydb_mark>#648#$\{ -{\bf g}_0,\cdots,-{\bf g}_n,\cdots\}$
<tex2html_mydb_mark>#649#$\{{\bf d}_0,\cdots,
{\bf d}_{N-1}\}$
<tex2html_mydb_mark>#650#${\bf d}_i^T{\bf Ad}_j=0\;(i\ne j)$
<tex2html_mydb_mark>#651#\begin{equation}
{\bf x}_{n+1}={\bf x}_n+\delta_n{\bf d}_n
  =\cdots ={\bf x}_0+\sum_{i=0}^n \delta_i{\bf d}_i
\end{equation}
<tex2html_mydb_mark>#652#${\bf x}^*={\bf x}_{n+1}-{\bf e}_{n+1}={\bf x}_n-{\bf e}_n$
<tex2html_mydb_mark>#653#\begin{equation}
{\bf e}_{n+1}={\bf e}_n+\delta_n{\bf d}_n
  =\cdots={\bf e}_0+\sum_{i=0}^n \delta_i{\bf d}_i
\end{equation}
<tex2html_mydb_mark>#654#$f({\bf x})={\bf x}^T{\bf Ax}/2-{\bf b}^T{\bf x}+c$
<tex2html_mydb_mark>#655#\begin{equation}
{\bf g}_i=f'({\bf x}_i)
  ={\bf A}{\bf x}_i-{\bf b}={\bf A}({\bf x}^*+{\bf e}_i)-{\bf b}
  ={\bf A}{\bf x}^*-{\bf b}+{\bf A}{\bf e}_i={\bf A}{\bf e}_i
\end{equation}
<tex2html_mydb_mark>#656#${\bf g}({\bf x}^*)={\bf A}{\bf x}^*-{\bf b}={\bf0}$
<tex2html_mydb_mark>#657#\begin{eqnarray}
\delta_i&=&-\frac{{\bf g}_i^T{\bf d}_i}{{\bf d}_i^T{\bf A}{\bf d}_i}
  =-\frac{{\bf e}_i^T{\bf A}{\bf d}_i}{{\bf d}_i^T{\bf A}{\bf d}_i}
  =-\frac{{\bf d}_i^T{\bf A}{\bf e}_i}{{\bf d}_i^T{\bf A}{\bf d}_i}
  =-\frac{ {\bf d}^T_i{\bf A} \left( {\bf e}_0+\sum_{j=0}^{i-1}\delta_j{\bf d}_j \right)}{{\bf d}^T_i{\bf A}{\bf d}_i}
  \nonumber\\
  &=&-\frac{ {\bf d}^T_i{\bf A}{\bf e}_0+\sum_{j=0}^{i-1}\delta_j{\bf d}^T_i{\bf A}{\bf d}_j}{{\bf d}^T_i{\bf A}{\bf d}_i}
  =-\frac{ {\bf d}^T_i{\bf A}{\bf e}_0}{ {\bf d}^T_i{\bf A}{\bf d}_i}

\end{eqnarray}
<tex2html_mydb_mark>#658#${\bf d}^T_i{\bf A}{\bf d}_j=0$
<tex2html_mydb_mark>#659#\begin{equation}
{\bf e}_{n+1}={\bf e}_n+\delta_n{\bf d}_n
  ={\bf e}_n-\left(\frac{{\bf d}_n^T{\bf A}{\bf e}_0}{{\bf d}_n^T{\bf A}{\bf d}_n}\right){\bf d}_n
\end{equation}
<tex2html_mydb_mark>#660#${\bf e}_0={\bf x}_0-{\bf x}^*$
<tex2html_mydb_mark>#661#$\{{\bf d}_0,\cdots,{\bf d}_{N-1}\}$
<tex2html_mydb_mark>#662#\begin{equation}
{\bf e}_0=\sum_{i=0}^{N-1} c_i{\bf d}_i
  =\sum_{i=0}^{N-1}{\bf p}_{{\bf d}_i}({\bf e}_0)
  =\sum_{i=0}^{N-1} \left(\frac{ {\bf d}^T_i{\bf A}{\bf e}_0}{ {\bf d}^T_i{\bf A}{\bf d}_i}\right){\bf d}_i

\end{equation}
<tex2html_mydb_mark>#663#${\bf p}_{{\bf d}_i}({\bf e}_0)$
<tex2html_mydb_mark>#664#\begin{equation}
{\bf p}_{{\bf d}_i}({\bf e}_0)=c_i{\bf d}_i
  =\left(\frac{ {\bf d}^T_i{\bf A}{\bf e}_0}{ {\bf d}^T_i{\bf A}{\bf d}_i}\right)
  {\bf d}_i
\end{equation}
<tex2html_mydb_mark>#665#\begin{equation}
c_i=\frac{{\bf d}^T_i{\bf A}{\bf e}_0}{{\bf d}^T_i{\bf A}{\bf d}_i}=-\delta_i
\end{equation}
<tex2html_mydb_mark>#666#${\bf e}_{n+1}$
<tex2html_mydb_mark>#667#\begin{equation}
{\bf e}_{n+1}={\bf e}_0+\sum_{i=0}^n\delta_i{\bf d}_i
  =\sum_{i=0}^{N-1}c_i{\bf d}_i-\sum_{i=0}^n c_i{\bf d}_i=\sum_{i=n+1}^{N-1}c_i{\bf d}_i
  =\sum_{i=n+1}^{N-1}{\bf p}_{{\bf d}_i}({\bf e}_0)
\end{equation}
<tex2html_mydb_mark>#668#${\bf p}_{{\bf d}_n}({\bf e}_0)$
<tex2html_mydb_mark>#669#${\bf e}_N={\bf0}$
<tex2html_mydb_mark>#670#${\bf x}_N={\bf x}^*+{\bf e}_N={\bf x}^*$
<tex2html_mydb_mark>#671#${\bf d}_k^T{\bf A}\;(k\le n)$
<tex2html_mydb_mark>#672#\begin{equation}
{\bf d}_k^T{\bf A}{\bf e}_{n+1}={\bf d}_k^T{\bf g}_{n+1}
  =\sum_{i=n+1}^{N-1}c_i{\bf d}_k^T{\bf A}{\bf d}_j=0
\end{equation}
<tex2html_mydb_mark>#673#${\bf e}_{n+1}$
<tex2html_mydb_mark>#674#${\bf d}_0,\cdots,{\bf d}_n$
<tex2html_mydb_mark>#675#${\bf g}_{n+1}$
<tex2html_mydb_mark>#676#$-{\bf g}_0$
<tex2html_mydb_mark>#677#$-{\bf g}_1$
<tex2html_mydb_mark>#678#$\{{\bf d}_0,\cdots,{\bf d}_{N-1}\}$
<tex2html_mydb_mark>#679#$\{{\bf v}_0,\cdots,{\bf v}_{N-1}\}$
<tex2html_mydb_mark>#680#\begin{equation}
{\bf d}_n={\bf v}_n-\sum_{j=0}^{n-1} {\bf p}_{{\bf d}_j}({\bf v}_n)
  ={\bf v}_n-\sum_{m=0}^{n-1} \left(\frac{{\bf d}^T_m{\bf A}{\bf v}_n}{{\bf d}^T_m{\bf A}{\bf d}_m}\right)  {\bf d}_m
  ={\bf v}_n-\sum_{m=0}^{n-1} \beta_{nm}{\bf d}_m
\end{equation}
<tex2html_mydb_mark>#681#$\beta_{nm}={\bf d}^T_m{\bf A}{\bf v}_n/({\bf d}^T_m{\bf A}{\bf d}_m)$
<tex2html_mydb_mark>#682#$\beta_{nm}{\bf d}_m$
<tex2html_mydb_mark>#683#${\bf v}_n=-{\bf g}_n$
<tex2html_mydb_mark>#684#\begin{equation}
{\bf d}_n =-{\bf g}_n-\sum_{m=0}^{n-1} \beta_{nm}{\bf d}_m
\end{equation}
<tex2html_mydb_mark>#685#\begin{equation}
\beta_{nm}=\frac{{\bf d}_m^T{\bf A}{\bf v}_n}{{\bf d}^T_m{\bf A}{\bf d}_m}
  =-\frac{{\bf d}_m^T{\bf A}{\bf g}_n}{{\bf d}^T_m{\bf A}{\bf d}_m}
  \;\;\;\;\;\;(m<n)
\end{equation}
<tex2html_mydb_mark>#686#${\bf d}_0,\cdots,{\bf d}_n$
<tex2html_mydb_mark>#687#\begin{equation}
{\bf g}_n=\sum_{i=0}^n \alpha_i{\bf d}_i
\end{equation}
<tex2html_mydb_mark>#688#${\bf g}_{n+1}^T$
<tex2html_mydb_mark>#689#\begin{equation}
{\bf g}_{n+1}^T{\bf g}_k={\bf g}_{n+1}^T\left( \sum_{i=0}^k \alpha_i{\bf d}_i\right)
  =\sum_{i=0}^k \alpha_i\;{\bf g}_{n+1}^T{\bf d}_i=0
\end{equation}
<tex2html_mydb_mark>#690#${\bf g}_{n+1}^T{\bf d}_k=0$
<tex2html_mydb_mark>#691#${\bf g}_{n+1}$
<tex2html_mydb_mark>#692#${\bf g}_0,\cdots,{\bf g}_n$
<tex2html_mydb_mark>#693#${\bf g}_k^T (k\ge n)$
<tex2html_mydb_mark>#694#\begin{equation}
{\bf g}_k^T{\bf d}_n=-{\bf g}_k^T{\bf g}_n-\sum_{m=0}^{n-1} \beta_{mn}\,{\bf g}_k^T{\bf d}_m
  =-{\bf g}_k^T{\bf g}_n
  =\left\{ \begin{array}{cc}-||{\bf g}_n||^2&n=k\\0&n<k\end{array}\right.
\end{equation}
<tex2html_mydb_mark>#695#${\bf g}_k^T{\bf d}_m=0$
<tex2html_mydb_mark>#696#${\bf g}_n^T{\bf d}_n=-||{\bf g}_n||^2$
<tex2html_mydb_mark>#697#\begin{equation}
\delta_n=-\frac{{\bf g}_n^T{\bf d}_n}{{\bf d}_n^T{\bf A}{\bf d}_n}
  =\frac{||{\bf g}_n||^2}{{\bf d}_n^T{\bf A}{\bf d}_n}
\end{equation}
<tex2html_mydb_mark>#698#\begin{equation}
{\bf g}_{m+1}={\bf A}{\bf x}_{m+1}-{\bf b}
  ={\bf A}({\bf x}_m+\delta_m{\bf d}_m)-{\bf b}
  =({\bf A}{\bf x}_m-{\bf b})+\delta_m{\bf A}{\bf d}_m
  ={\bf g}_m+\delta_m{\bf A}{\bf d}_m
\end{equation}
<tex2html_mydb_mark>#699#${\bf g}_n^T$
<tex2html_mydb_mark>#700#\begin{equation}
{\bf g}_n^T{\bf g}_{m+1}={\bf g}_n^T{\bf g}_m+\delta_m{\bf g}_n^T{\bf A}{\bf d}_m
  =\delta_m{\bf g}_n^T{\bf A}{\bf d}_m 

\end{equation}
<tex2html_mydb_mark>#701#${\bf g}_n^T{\bf g}_m=0$
<tex2html_mydb_mark>#702#${\bf g}_n^T{\bf A}{\bf d}_m$
<tex2html_mydb_mark>#703#\begin{equation}
{\bf g}_n^T{\bf A}{\bf d}_m
  =\frac{1}{\delta_m} {\bf g}_n^T{\bf g}_{m+1}
  =\left\{\begin{array}{cl}
  ||{\bf g}_n||^2/\delta_{n-1}&m=n-1\\0&m<n-1\end{array}\right.
\end{equation}
<tex2html_mydb_mark>#704#\begin{equation}
\beta_{nm}=-\frac{{\bf d}_m^T{\bf A}{\bf g}_n}{{\bf d}^T_m{\bf A}{\bf d}_m}
  =\left\{\begin{array}{cl}-||{\bf g}_n||^2/\delta_{n-1}{\bf d}_{n-1}^T{\bf A}{\bf d}_{n-1}
&m=n-1
  \\0&m<n-1\end{array}\right.
\end{equation}
<tex2html_mydb_mark>#705#${\bf v}_n=-{\bf g}_n$
<tex2html_mydb_mark>#706#$\beta_{nm}$
<tex2html_mydb_mark>#707#$\delta_{n-1}=||{\bf g}_{n-1}||^2/{\bf d}_{n-1}^T{\bf A}{\bf d}_{n-1}$
<tex2html_mydb_mark>#708#$\beta_{nm}=\beta_m$
<tex2html_mydb_mark>#709#\begin{equation}
\beta_n=-\frac{||{\bf g}_n||^2}{||{\bf g}_{n-1}||^2}
\end{equation}
<tex2html_mydb_mark>#710#\begin{equation}
{\bf d}_0=-{\bf g}_0
\end{equation}
<tex2html_mydb_mark>#711#$\varepsilon=||{\bf g}_n||^2$
<tex2html_mydb_mark>#712#\begin{equation}
\delta_n=\frac{||{\bf g}_n||^2}{{\bf d}_n^T{\bf A}{\bf d}_n},
  \;\;\;\;\;\;\;\;
  {\bf x}_{n+1}={\bf x}_n+\delta_n{\bf d}_n
\end{equation}
<tex2html_mydb_mark>#713#\begin{equation}
{\bf g}_{n+1}=\frac{d}{d{\bf x}}f({\bf x}_{n+1})
\end{equation}
<tex2html_mydb_mark>#714#\begin{equation}
